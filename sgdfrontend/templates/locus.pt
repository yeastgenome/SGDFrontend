<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>
	
<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="row">
		<h1 class="head-alt"><span tal:content="bioent['full_name']"></h1>
		<p class="lead">
			<span tal:content="bioent['description']"></span>
		</p>
	</div>
</header>

<!-- Docs nav
================================================== -->
<div class="row">

	<div id="sidebar" class="large-3 columns">
		<ul class="side-nav" id='navbar' name='navbar' style="margin-left:auto; margin-right:auto">
			<li><h3><span tal:content="bioent['full_name']"></h3></li>
			<li><a href="#summary"><i class="icon-chevron-right"></i> Summary</a></li>
          	<li><a href="#go"><i class="icon-chevron-right"></i> Gene Ontology</a></li>
          	<li><a href="#phenotype"><i class="icon-chevron-right"></i> Phenotype</a></li>
          	<li><a href="#interactions"><i class="icon-chevron-right"></i>Interaction</a></li>
          	<li><a href="#wiki"><i class="icon-chevron-right"></i> Wiki</a></li>
          	<li><a href="#litguide"><i class="icon-chevron-right"></i> References</a></li>
		</ul>
	</div>
	
	<div class="large-9 columns">  

<!-- Summary
================================================== -->
<section id="summary">
	<div class="page-header">
		<h1>Summary</h1>
		<div>
			<ul class="unstyled">
				<li><strong>Standard Name: </strong><span tal:content="bioent['display_name']"></li>
				<li><strong>Systematic Name: </strong><span tal:content="bioent['format_name']"></li>
				<br>
				<li tal:condition="python: bioent['aliases'] != ''"><strong>Aliases: </strong><span tal:content="bioent['aliases']"></li>
				<li><strong>Type: </strong><span tal:content="bioent['bioent_type']"></li>
				<li tal:condition="python: bioent['qualifier']"><strong>Qualifier: </strong><span tal:content="bioent['qualifier']"></li>
				<br>
				<li tal:condition="python: bioent['source']"><strong>Source: </strong><span tal:content="bioent['source']"></li>
				<li tal:condition="python: bioent['attribute']"><strong>Attribute: </strong><span tal:content="bioent['attribute']"></li>
				<li tal:condition="python: bioent['name_description']"><strong>Name Derived From: </strong><span tal:content="bioent['name_description']"></li>
			</ul>
		</div>
	</div>
</section>

<!-- GO
================================================== -->
<section id="go">
	<div class="page-header">
		<h1>Gene Ontology <span id='go_header'></span></h1>
		<a tal:attributes="href go_evidence_link"><button class="radius">All Gene Ontology Evidence <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
		<p>* There are <span id='go_comp_message'></span> computational go terms curated for <span tal:content="bioent['display_name']"></span>.</p>
		
		<h3>Molecular Function <span id='go_f_header'></span></h3>
		<span id='go_f_message' class="hide">No known molecular function gene ontology associations.</span>
		<div id='go_f_wrapper'>	
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='go_f_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Description</th>
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>		
		<br></br>

		<h3>Biological Process <span id='go_p_header'></span></h3>
		<span id='go_p_message' class="hide">No known biological process gene ontology associations.</span>
		<div id='go_p_wrapper'>	
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='go_p_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Description</th>
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>	
		<br></br>
		
		<h3>Cellular Component <span id='go_c_header'></span></h3>
		<span id='go_c_message' class="hide">No known cellular component gene ontology associations.</span>
		<div id='go_c_wrapper'>	
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='go_c_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Description</th>    
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>
	</div>
</section>

<!-- Phenotype
================================================== -->
<section id="phenotype">
	<div class="page-header">
		<h1>Phenotype <span id='phenotype_header'></span></h1> 
		<a tal:attributes="href phenotype_evidence_link"><button class="radius">All Phenotype Evidence <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
		<!-- Phenotype Table
		========================================== -->
		<h3>Cellular Phenotype <span id='cellular_phenotype_header'></span></h3>
		<div>
			<span id='phenotype_message' class="hide">No known phenotypes.</span>
			<div id='phenotype_wrapper'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='phenotype_table' width="100%">
				<thead>
  				<tr>
  					<th>Phenotype</th>
  					<th>Gene</th>
  					<th>Qualifier</th>
  					<th>Mutant Type</th>
  				</tr>
  				</thead>
				</table>
			</div>
		</div>
		<br></br>
		<!-- Chemical Phenotype Table
		========================================== -->
		<h3>Chemical Phenotype <span id='chemical_phenotype_header'></span></h3>
		<span>* Chemical sensitivity information is not curated.</span>
		<div>
			<span id='chemical_phenotype_message'  class="hide">No known chemical phenotypes.</span>
			<div id='chemical_phenotype_wrapper'>
								<table class='table table-striped table-bordered table-condensed tablesorter' id='chemical_phenotype_table' width="100%">
			<thead>
  			<tr>
   				<th>Phenotype</th>
  				<th>Gene</th>
  				<th>Chemical</th>
  				<th>Qualifier</th>  				
  				<th>Mutant Type</th>
  			</tr>
  			</thead>
 
			</table>
		</div>
		<br></br>
		<!-- Protein/Peptide and RNA Interactions Table
		========================================== -->
		<h3>Protein/Peptide and RNA Phenotype <span id='pp_rna_phenotype_header'></span></h3>
		<div>
			<span id='pp_rna_phenotype_message' class="hide">No known protein/peptide and RNA phenotypes.</span>
			<div id='pp_rna_phenotype_wrapper'>				
				<table class='table table-striped table-bordered table-condensed tablesorter' id='pp_rna_phenotype_table' width="100%">
			<thead>
  			<tr>
  				<th>Phenotype</th>
  				<th>Gene</th>
  				<th>Reporter</th>
  				<th>Qualifier</th>
  				<th>Mutant Type</th>
  			</tr>
  			</thead>
 
			</table>
		</div>

	</div>
</section>

<!-- Interactions
================================================== -->
<section id="interactions">
	<div class="page-header">
		<h1>Interaction <span id='interaction_header'></span></h1>
		<a tal:attributes="href interaction_evidence_link"><button class="radius">All Interaction Evidence <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
		
		<div>
		<h3>Cytoscape Visualization</h3>
		<strong>Filter Interactions by # Total Evidence:</strong>
		<br></br>
		<div id="slider-range-min"></div>
		<br></br>
		<div id="cytoscapeweb">
            Cytoscape visualization is loading...
        </div>
        <span>Export:</span>
        <div class="btn-group">
        	<a class="btn" onclick="vis.exportNetwork('png', '/download_graph/png')">png</a>
        	<a class="btn" onclick="vis.exportNetwork('svg', '/download_graph/svg')">svg</a>
        	<a class="btn" onclick="vis.exportNetwork('pdf', '/download_graph/pdf')">pdf</a>
        	<a class="btn" onclick="vis.exportNetwork('xgmml', '/download_graph/xml')">xgmml</a>
        	<a class="btn" onclick="vis.exportNetwork('graphml', '/download_graph/txt')">graphml</a>
        	<a class="btn" onclick="vis.exportNetwork('sif', '/download_graph/txt', options={nodeAttr:'label', interactionAttr:'label'})">txt</a>

        </div>
        </div>
        <br></br>
		<span id='interaction_message' class="hide">No known interactions.</span>
		<div id='interaction_wrapper'>					
			<ul class='unstyled'>
				<table class='table table-striped table-bordered table-condensed tablesorter' id='interaction_table' width="100%">
			<thead>
  			<tr>
    			<th>Interactor</th>
    			<th>Interactor</th>
    			<th>Genetic Evidence</th>
    			<th>Physical Evidence</th>
    			<th>Total Evidence</th>
  			</tr>
  			</thead>
  			
			</table>
			</ul>
		</div>

	</div>
</section>

<!-- Wiki
================================================== -->
<section id="wiki">
	<div class="page-header">
		<h1>Wiki</h1>
    	<span tal:content="structure bioent.wiki_name_with_link"></span>
	</div>
</section>

<!-- LitGuide
================================================== -->
<section id="litguide">
	<div class="page-header">
		<h1>References <span id='litguide_header'></span></h1>
		<a tal:attributes="href bioent_evidence_link"><button class="radius">All References (Lit-guide) <i class="icon-chevron-right icon-white"></i></button></a>
		<br></br>
				
		<span id='litguide_message' class="hide">No litguide information curated.</span>
		<div id='litguide_wrapper'>					
			<ul id='reference_list'>
			</ul>
		</div>

	</div>
</section>


<script>

	$(document).ready(function() {

		//Get go data

  		$.getJSON("${go_overview_table_link}", function(data) {
			var go_c = data['go_c'];
  			var go_p = data['go_p'];
  			var go_f = data['go_f'];
  			var comp_count = data['computational'];
  			
			//Set total go count
			var num_rows = go_c.length + go_p.length + go_f.length;
			document.getElementById("go_header").innerHTML = '(' + num_rows + ')';
			
  			//Set up function table
			var options = basic_datatable_options("go_f_header", "${go_f_filename}", "go_f_wrapper", "go_f_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = go_f;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null]
			$('#go_f_table').dataTable(options);
			
			//Set up process table
			var options = basic_datatable_options("go_p_header", "${go_p_filename}", "go_p_wrapper", "go_p_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = go_p;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null]
			$('#go_p_table').dataTable(options);
		
			//Set up component table
			var options = basic_datatable_options("go_c_header", "${go_c_filename}", "go_c_wrapper", "go_c_message");
			options["aaSorting"] = [[0, "asc"]];
			options["aaData"] = go_c;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null]
			$('#go_c_table').dataTable(options);
			
			//Set up computational message
			document.getElementById("go_comp_message").innerHTML = comp_count;
  		});
			
  		//Get phenotype data
  		$.getJSON("${phenotype_overview_table_link}", function(data) {
  			var phenotype_table = data['cellular_pheno'];
  			var chemical_phenotype_table = data['chemical_pheno'];
  			var pp_rna_phenotype_table = data['pp_rna_pheno'];
  			
			//Set total phenotype count
			var num_rows = phenotype_table.length + chemical_phenotype_table.length + pp_rna_phenotype_table.length;
			document.getElementById("phenotype_header").innerHTML = '(' + num_rows + ')';
			
  			//Set up cellular phenotype table
			var options = basic_datatable_options("cellular_phenotype_header", "${cellular_phenotype_filename}", "phenotype_wrapper", "phenotype_message");
			options["aaSorting"] = [[0, "asc"], [1, 'asc']];
			options["aaData"] = phenotype_table;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, null, null];
			$('#phenotype_table').dataTable(options);
		
			//Set up chemical phenotype table
			var options = basic_datatable_options("chemical_phenotype_header", "${chemical_phenotype_filename}", "chemical_phenotype_wrapper", "chemical_phenotype_message");
			options["aaSorting"] = [[1, "asc"]];
			options["aaData"] = chemical_phenotype_table;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, null, null, null];
			$('#chemical_phenotype_table').dataTable(options);
		
			//Set up pp-rna phenotype table
			var options = basic_datatable_options("pp_rna_phenotype_header", "${pp_rna_phenotype_filename}", "pp_rna_phenotype_wrapper", "pp_rna_phenotype_message");
			options["aaSorting"] = [[1, "asc"]];
			options["aaData"] = pp_rna_phenotype_table;
			options["aoColumns"] = [null, {"bSearchable":false, "bVisible":false}, null, null, null];
			$('#pp_rna_phenotype_table').dataTable(options);
  		});
  		
  		//Set up interaction table
		var options = basic_datatable_options("interaction_header", "${interaction_filename}", "interaction_wrapper", "interaction_message");
		options["aaSorting"] = [[4, "desc"]];
		options["aoColumns"] = [{"bSearchable":false, "bVisible":false}, null, null, null, {"sType":"num-html"}];
		options["bProcessing"] = true;
		options["sAjaxSource"] = "${interaction_overview_table_link}";
		options["bPaginate"] = true;
		$('#interaction_table').dataTable(options);
		
		//Set up litguide
		$.getJSON("${bioent_overview_table_link}", function(data) {
  			var references = data;
			var ref_list = document.getElementById("reference_list");
			for (var i=0; i < references.length; i++) {
				var citation = references[i];
				var p=document.createElement('p');
				p.innerHTML = citation;
				ref_list.appendChild(p);
			}
		});
		
	} );
</script>



<script>
	var vis = null;
	window.onload=function() {
		vis = setup_interaction_cytoscape_vis("${interaction_graph_link}");   
	};           
</script>
        
<style>
	/* The Cytoscape Web container must have its dimensions set. */
	html, body { height: 80%; width: 100%; padding: 0; margin: 0; }
	#cytoscapeweb { width: 100%; height: 100%; }
</style>

</div>

</body>
</div>
</div>

