{% extends "global_layout.jinja2" %}

{% block title %}Literature Recently Added to SGD{% endblock %}

{% block head %}

<script>
  var references = {{ references_js|safe }};
</script>

{% endblock %}

{% block body %}

<!-- Navbar
================================================== -->
<div class="row">
	
    <!-- Includes navbar.html into template -->
    {% include "navbar.jinja2" %}

    <div class="small-12 medium-10 columns content-column">

        <script>
            add_navbar_title("Literature Recently Added to SGD");
            add_navbar_element('References', 'references');
        </script>

<!-- References
================================================== -->
        <section id="references" data-magellan-destination="references">
            <div class="page-header">
                <h1>Literature Recently Added to SGD</h1>
                <h2 class="subheader" id="dates"></h2><span id="references_header" class="label secondary round"></span></h2>
                <hr />
                {% set reference_list_id = "references_list" %}
                {% set empty_message = "No literature added recently." %}
                {% include "reference_list_with_entities.jinja2" %}
            </div>
        </section>
    </div>
</div>
{% endblock body %}

{% block scripts %}
<script src="{{asset_root}}/js/references_this_week.js">
{% endblock scripts %}

