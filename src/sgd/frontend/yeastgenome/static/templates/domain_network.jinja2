{% extends "network.jinja2" %}

{% block help %}
    Click on a gene or domain name to go to its specific page within SGD; drag any of the gene or domain objects around
    within the visualization for easier viewing; click “Reset” to automatically redraw the diagram.
{% endblock %}

{% block legend %}{% endblock %}

{% block additional_buttons %}
    <ul class="button-group radius">
        <li><a id="{{ network_id }}_txt_download" class="small button secondary"><i class="fa fa-download"></i> Download
            Shared Domains (.txt)</a></li>
        <li>&nbsp &nbsp</li>
	<li><a id="{{ network_id }}_shared_domain_gene_analyze" class="small button secondary" disabled="true"><i class='fa fa-briefcase'></i> Analyze</a>
    </ul>
{% endblock %}
